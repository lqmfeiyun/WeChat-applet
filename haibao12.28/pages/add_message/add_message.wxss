page {
  background-color: #eee;
}

.content {
  margin-top: 15px;
  background-color: #fff;
}

.content_line {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #d3d3d3;
}

.content_line text {
  padding: 10px;
  margin: auto 0;
  font-size: 13px;
  /*line-height: 40px;*/
  color: #999;
}

.content_line_right {
  margin: 10px;
  display: flex;
}

.content_line_right text {
  font-size: 15px;
}

.content_line_right .sj_right {
  height: 10px;
  width: 10px;
  margin: auto 0;
}
.content_line_right input{
    font-size: 15px;
    text-align: right;
}
.note{
  display: block;
  padding: 10px;
  font-size: 11px;
  /*line-height: 20px;*/
  color: #3d3d3d;
}

.click button{
  color: cornflowerblue;
}

picker{
  justify-content: space-between;
}