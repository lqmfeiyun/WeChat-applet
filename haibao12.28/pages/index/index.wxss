.point{
  display: flex;
  justify-content: center;
  padding-bottom: 20px;
}
.point_style{
  display: inline-block;
  margin: 0 2px;
  width: 10px;
  height: 10px;
  background-color: #fff;
  border-radius: 10px;
}
/*.content{
  margin: 10px auto;
}*/
.content1{
  display: flex;
  width: 100%;
  padding-top: 20px;
  text-align: center;
  align-items: center;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}
.content1_style{
  margin: 0 auto;
}
.content1_style image{
  width: 20px;
  height: 20px;
}
.content1_style_content,
.footer_style_content{
  font-family: 'Helvetica Neue',Helvetica,tahoma,arial;
  margin-bottom: 20px;
  font-size: 14px;
}
.content2,
.content7{
  display: -webkit-box;
  -webkit-box-align: center;
  width: 100%;
  background-color: #fff;
}
.content2_title,
.content7_title{
  padding:10px 20px;
  color: dodgerblue;
  font-weight: bolder;
  font-style: italic;
}
.content2_content,
.content7_content{
  display: -webkit-box;
  -webkit-box-align: center;
}
.content2_content image,
.content7_content image{
  width: 20px;
  height:20px;
}
.content2_content view,
.content7_content view{
  padding: 5px;
  width: 220px;
  line-height: 25px;
  word-break:break-all;  
  word-wrap: break-word;
}
.content3{
  margin: 10px 0;
  width: 100%;
  height: 80px;
  background-color: #fff;
}
.content3 .swiper image{
  height: 80px;
  width: 100%;
}
.content4{
  margin: 10px 0;
  width: 100%;
  height: 50px;
  background-color: #fff;
}
.content5,
.content6{
  margin: 10px 0;
  width: 100%;
  height: 50px;
  white-space:nowrap;
  background-color: #fff;
}
.content4_pic{
  padding: 10px 0;
  display: inline-block;
  border-left:1px solid #f0f0f0;
  border-right: 1px solid #f0f0f0;
}
.content4_pic image,
.content6 image{
  display: inline-block;
  height: 25px;
  width: 80px;
}

.footer_box{
  position: relative;
  height: 70px;
}
.footer{
  position: fixed;
  width: 100%;
  bottom: 0;
  background-color: #fff;
  border-top: 1px solid #f0f0f0;
  border-bottom: none;
}
.footer ul{
  display: flex;
  /*padding: 0 20px;*/
  align-items: center;
  justify-content: space-between;
}
.footer_style{
  padding-top: 10px;
  width: 30%;
  display: inline-block;
  text-align: center;
}
.footer_style image{
  width: 20px;
  height: 20px;
}
.footer ul li:hover{
  background-color:#e41a3b;
}

.fa-home{
  color: #38bcd2;
}


/*修改swiper原点样式*/
page .wx-swiper-dot{
    width: 18rpx;
    display: inline-flex;
    height: 18rpx;
    margin-right: 18rpx;
}
page .wx-swiper-dot::before{
    content: '';
    background: #8c8c8f;
    flex-grow: 1; 
    border-radius: 100%;
}
page .wx-swiper-dot-active::before{
    content: '';
    background:#fff;
    flex-grow: 1;
    border-radius: 100%;
}