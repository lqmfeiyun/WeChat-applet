.first_page{
  width: 100%;
  height: 100%;
  background-color: #eee;
}
.title{
  font-size: 20px;
  line-height: 40px;
  text-align: center;
}
.top{
  text-align: center;
  background-image: url(https://wx.haibaobaoxian.com/hb-biz/main_sg/images/bg1.png);
  background-repeat: no-repeat;
  background-position:center;
  background-size: 100% 100%;
  background-color: #274e70;
}
.top navigator{
  width: 100px;
  margin: 5px auto;
  border-radius: 50%;
  z-index: 99;
}
.top image{
  display: -webkit-box;
  width: 100px;
  height: 100px;
  margin: 5px auto;
  border-radius: 50%;
}
.swiper{
  height: 160px;
}
.swiper text{
  margin: 10px auto;
  font-size: 15px;
  line-height: 40px;
  color: #fff;
}
.swiper .bt1{
  margin: -40px auto 0 auto;
  width: 20%;
  background-color: #32fe23;
  border-radius: 10px;
}
.swiper .contect1{
  /*margin: 10px auto;*/
  font-size: 15px;
  line-height: 30px;
  color: #fff;
}

/*滚动*/
.swiper_top{
  white-space:nowrap;
}

